macro_screen_shot: macro_screen_shot {
        compatible = "zmk,behavior-macro";
        label = "macro_screen_shot";
        #binding-cells = <0>;
        wait-ms = <30>;
        tap-ms = <30>;
        bindings =
            = <&macro_press &kp LC>
            , <&macro_press &kp LS>
            , <&macro_tap &kp S>
            , <&macro_release &kp LS>
            , <&macro_release &kp LC>;
    };
macro_emoji: macro_emoji {
        compatible = "zmk,behavior-macro";
        label = "macro_emoji";
        #binding-cells = <0>;
        wait-ms = <30>;
        tap-ms = <30>;
        bindings =
            = <&macro_press &kp LC>
            , <&macro_press &kp LG>
            , <&macro_tap &kp SPACE>
            , <&macro_release &kp LG>
            , <&macro_release &kp LC>;
    };
